.mh_me_aanimation_01 {
    animation: mobile 2s ease-in-out;
    opacity: 0;
}

@keyframes mobile {
    0% {
        transform: translate(-50%, 50%) scale(0.2, 0.2);
        opacity: 0;
    }
    25% {
        transform: translate(-50%, 50%) scale(1.2, 1.2);
        opacity: 1;
    }
    30% {
        transform: translate(-50%, 50%) scale(1.2, 1.2);
        opacity: 1;
    }
    60% {
        transform: translate(-50%, 50%) scale(1, 1);
        opacity: 1;
    }
    60% {
        transform: translate(-50%, 50%) scale(1, 1) rotate(0deg);
        opacity: 1;
    }
    90% {
        transform: translate(-80%, 100%) scale(0.5, 0.5) rotate(350deg);
        opacity: 1;
    }
    100% {
        transform: translate(-100%, 160%) rotate(360deg) scale(0.1, 0.1);
        opacity: 0.5;
    }
}

